<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="assignmentAbout" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>argj_pt_BR</value></property>
      <property name="name"><value>Atividades</value></property>
      <property name="location"><value>/sakai_assignment_grades/argj_pt_BR.html</value></property>
      <property name="defaultForTool"><value>sakai.assignment</value></property>
   </bean>

   <bean id="assignmentOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardn_pt_BR</value></property>
      <property name="name"><value>Atividades - Resumo</value></property>
      <property name="location"><value>/sakai_assignment_grades/ardn_pt_BR.html</value></property>
   </bean>

   <bean id="createAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
         <property name="docId"><value>arbs_pt_BR</value></property>
      <property name="name"><value>Criar, Editar ou Deletar Atividades</value></property>
      <property name="location"><value>/sakai_assignment_grades/arbs_pt_BR.html</value></property>
   </bean>
   
   <bean id="gradingAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arda_pt_BR</value></property>
      <property name="name"><value>Nota da Atividade/Rever Situação do Aluno</value></property>
      <property name="location"><value>/sakai_assignment_grades/arda_pt_BR.html</value></property>
   </bean>
   
   <bean id="assignmentAttachment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfj_pt_BR</value></property>
      <property name="name"><value>Adicionar Anexos</value></property>
      <property name="location"><value>/sakai_assignment_grades/arfj_pt_BR.html</value></property>
   </bean>
  
   <bean id="reviseAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzj_pt_BR</value></property>
      <property name="name"><value>Ver Atividade Como Aluno</value></property>
      <property name="location"><value>/sakai_assignment_grades/aqzj_pt_BR.html</value></property>
   </bean>  
   <bean id="returnAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardd_pt_BR</value></property>
      <property name="name"><value>Retornar Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/ardd_pt_BR.html</value></property>
   </bean>   
   <bean id="resubmitAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arcy_pt_BR</value></property>
      <property name="name"><value>Reenviar Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/arcy_pt_BR.html</value></property>
   </bean>   
   <bean id="exportAssignmentGrade" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arcd_pt_BR</value></property>
      <property name="name"><value>Exportar Notas da Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/arcd_pt_BR.html</value></property>
   </bean>   
   <bean id="readAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aren_pt_BR</value></property>
      <property name="name"><value>Ler Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/aren_pt_BR.html</value></property>
   </bean>  
   <bean id="duplicateAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardj_pt_BR</value></property>
      <property name="name"><value>Duplicar Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/ardj_pt_BR.html</value></property>
   </bean>   
   <bean id="submitAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbm_pt_BR</value></property>
      <property name="name"><value>Completar e Enviar Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/arbm_pt_BR.html</value></property>
   </bean>   
   <bean id="postAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbk_pt_BR</value></property>
      <property name="name"><value>Postar Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/arbk_pt_BR.html</value></property>
   </bean>   
   <bean id="assignmentHonorPledge" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbi_pt_BR</value></property>
      <property name="name"><value>Adicionar Compromisso</value></property>
      <property name="location"><value>/sakai_assignment_grades/arbi_pt_BR.html</value></property>
   </bean>   
   <bean id="checkAssignmentGrade" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbg_pt_BR</value></property>
      <property name="name"><value>Verificar Nota da Atividade</value></property>
      <property name="location"><value>/sakai_assignment_grades/arbg_pt_BR.html</value></property>
   </bean>
   <bean id="assignmentGbMethods" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>avnd_pt_BR</value></property>
      <property name="name"><value>Link da Atividade no Quadro de Notas</value></property>
      <property name="location"><value>/sakai_assignment_grades/avnd_pt_BR.html</value></property>
   </bean>
   
    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="assignmentCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Atividades</value></property>
               <property name="resources">
                  <list>
                     <ref bean="assignmentAbout"/>
                     <ref bean="assignmentOverview"/>
                     <ref bean="createAssignment"/>
                     <ref bean="assignmentGbMethods"/>
					 <ref bean="gradingAssignment"/>
					 <ref bean="assignmentAttachment"/>
                     			 <ref bean="reviseAssignment"/>					 
					 <ref bean="returnAssignment"/>
					 <ref bean="resubmitAssignment"/>
					 <ref bean="exportAssignmentGrade"/>
					 <ref bean="readAssignment"/>
					 <ref bean="duplicateAssignment"/>
					 <ref bean="submitAssignment"/>
					 <ref bean="postAssignment"/>
					 <ref bean="assignmentHonorPledge"/>
					 <ref bean="checkAssignmentGrade"/>
                     
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
